{
  "name": "easy-ui-react-lib",
  "version": "1.0.0",
  "keywords": [
    "react",
    "library"
  ],
  "homepage": "https://github.com/aca-dec-2020/ui-lib",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "author": "Nvard Yeritsyan, Harutyun Qanaqaryan, Anthony Alexanyan",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.6.0",
    "@mdi/js": "^5.9.55",
    "@mdi/react": "^1.5.0",
    "@mdi/font": "^5.9.55"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "publish:npm": "babel src -d lib --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
